<div class="container" style="margin-top: 30px;">
 
  <h3 class="mt-10 text-primary">Estos son todos los productos</h3>
  <div class="table-responsive">
    <table class="table table-dark">
      <tr style="color: rgba(13, 110, 253);"> <th>Titulo</th> <th>Precio</th> <th>Imagen</th> </tr>
      {{#each data}}
        <tr>
          <td>{{this.titulo}}</td>
          <td> ${{this.precio}}</td> 
          <td><img src ={{this.thumbail}} width="40"height="40"></td>
        </tr>
      {{else}}
        <h3>No hay productos en la lista</h3>
      {{/each}}
    </div>
    <a href="/">Agregar un nuevo producto</a>
</div>